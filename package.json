{
  "name": "gulp-mdvars",
  "version": "0.0.2",
  "description": "Extract VarStream metadatas from markdown files.",
  "main": "src/index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha tests/*.mocha.js",
    "coveralls": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- tests/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover": "./node_modules/istanbul/lib/cli.js cover --report html ./node_modules/mocha/bin/_mocha -- tests/*.mocha.js -R spec -t 5000"
  },
  "keywords": [
    "gulpplugin",
    "gulp",
    "markdown",
    "varstream",
    "metadata"
  ],
  "homepage": "https://github.com/nfroidure/gulp-mdvars",
  "author": {
    "name": "Nicolas Froidure",
    "url": "http://www.insertafter.com/blog.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nfroidure/gulp-mdvars.git"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/gulp-mdvars/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/nfroidure/gulp-mdvars/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "gulp-util": "~2.2.0",
    "mdvars": "0.0.2",
    "readable-stream": "^1.0.26-2"
  },
  "devDependencies": {
    "event-stream": "~3.1.1",
    "mocha": "~1.18.2",
    "gulp": "~3.6.0",
    "mocha-lcov-reporter": "0.0.1",
    "coveralls": "~2.10.0",
    "istanbul": "~0.2.6",
    "mdvars": "0.0.2",
    "gulp-util": "~2.2.14"
  }
}
